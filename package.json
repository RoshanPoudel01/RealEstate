{
  "name": "real-state",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "start": "vite --host=0.0.0.0",
    "snippet": "npx @chakra-ui/cli snippet",
    "prestart:stage": "yarn",
    "lint": "yarn lint:fix && yarn lint:format",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.gitignore",
    "lint:format": "prettier  --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
    "cm": "cz",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "dependencies": {
    "@chakra-ui/react": "^3.2.2",
    "@ckeditor/ckeditor5-react": "^9.4.0",
    "@emotion/react": "^11.13.5",
    "@hookform/resolvers": "^3.9.1",
    "@phosphor-icons/react": "^2.1.7",
    "@tanstack/react-query": "^5.62.2",
    "@tanstack/react-table": "^8.20.5",
    "axios": "^1.6.7",
    "ckeditor5": "^44.0.0",
    "compressorjs": "^1.2.1",
    "convert": "^5.6.1",
    "html-react-parser": "^5.2.0",
    "i18next": "^24.1.0",
    "i18next-react": "^0.2.2",
    "lodash": "^4.17.21",
    "next-themes": "^0.4.3",
    "path": "^0.12.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.5",
    "react-error-boundary": "^4.0.12",
    "react-helmet-async": "^2.0.4",
    "react-hook-form": "^7.50.0",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^5.4.0",
    "react-multi-carousel": "^2.8.5",
    "react-router-dom": "^6.21.3",
    "react-select": "^5.8.3",
    "react-slick": "^0.30.2",
    "slick-carousel": "^1.8.1",
    "yup": "^1.5.0",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/lodash": "^4.17.13",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-i18next": "^8.1.0",
    "@types/react-slick": "^0.23.13",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.15.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.12.0",
    "husky": "^9.1.7",
    "prettier": "^3.4.2",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.15.0",
    "vite": "^6.0.1",
    "vite-plugin-checker": "^0.8.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-svgr": "^4.3.0",
    "vite-tsconfig-paths": "^5.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "description": "real state",
  "main": "index.js",
  "repository": "https://github.com/RoshanPoudel01/RealEstate.git",
  "author": "Roshan Poudel <1.rosanpoudel@gmail.com>",
  "license": "MIT"
}
